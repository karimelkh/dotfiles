XF86AudioRaiseVolume
	amixer set Master 5%+

XF86AudioLowerVolume
	amixer set Master 5%-

XF86AudioMute
	amixer set Master toggle

XF86MonBrightnessUp
	brightnessctl set 5%+

XF86MonBrightnessDown
	brightnessctl set 5%-

XF86Search
	firefox duckduckgo.com

XF86Explorer
	pcmanfm

Print
	flameshot gui

Menu
	rofi -show window

alt + w
	firefox

# alt + z
alt + shift + d
	zathura

alt + a
	anki

alt + shift + p
	rofi -show drun

alt + shift + w
	rofi -show window

super + shift + c
	$HOME/.config/dwm/scripts/toggle_conky.sh

super + l
	betterlockscreen -l dim
